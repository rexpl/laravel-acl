{
    "name": "rexpl/laravel-acl",
    "description": "Extensive acl for laravel, including groups and permissions.",
    "keywords": [
        "laravel",
        "groups",
        "acl",
        "permissions"
    ],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "rexpl",
            "email": "75545403+rexpl@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/contracts": "^10.0|^11.0",
        "staudenmeir/laravel-cte": "^1.9"
    },
    "require-dev": {
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Rexpl\\LaravelAcl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rexpl\\LaravelAcl\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rexpl\\LaravelAcl\\ServiceProvider\\AclServiceProvider"
            ]
        }
    }
}
